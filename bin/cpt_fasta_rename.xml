<tool force_history_refresh="false" hidden="false" id="cpt_fasta_rename.py" name="cpt_fasta_rename.py" workflow_compatible="true">
  <description>rename fasta sequences</description>
  <stdio>
    <exit_code level="fatal" range="1:"/>
  </stdio>
  <command interpreter="python"><![CDATA[cpt_fasta_rename.py $positional_1
$positional_2
> $default]]></command>
  <inputs>
    <param label="fasta file" name="positional_1" type="data"/>
    <param area="false" label="New name for the fasta sequence" name="positional_2" type="text"/>
  </inputs>
  <outputs>
    <data format="txt" hidden="false" name="default"/>
  </outputs>
  <help><![CDATA[TODO: Bug @erasche until he fixes it or make a PR]]></help>
</tool>

