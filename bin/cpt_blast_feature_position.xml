<?xml version="1.0"?>
<tool id="cpt_blast_feature_position.py" name="Blast-Gbk Location Finder" version="1.0">
  <description>Match blast hits to positions in Genbank file</description>
  <stdio>
    <exit_code level="fatal" range="1:"/>
  </stdio>
  <version_command>python cpt_blast_feature_position.py --version</version_command>
  <command interpreter="python"><![CDATA[cpt_blast_feature_position.py
$positional_1
$positional_2
> $default]]></command>
  <inputs>
    <param label="Genbank Database file" name="positional_1" type="data"/>
    <param label="12+ Column Blast Results" name="positional_2" type="data"/>
  </inputs>
  <outputs>
    <data format="tabular" hidden="false" name="default"/>
  </outputs>
  <help><![CDATA[
Given 12+ column blast results, find the positions of features with matching
IDs from a genbank file which was used to generate the blast database
      ]]></help>
</tool>
