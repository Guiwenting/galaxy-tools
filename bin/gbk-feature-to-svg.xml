<?xml version="1.0"?>
<tool id="edu.tamu.cpt.genbank.DavidsonPlot" name="Genbank Feature Array Plot" version="1.94">
  <description>plots features as an array of colored boxes</description>
  <version_command>python gbk-feature-to-svg.py --version</version_command>
  <stdio>
    <exit_code level="fatal" range="1:"/>
  </stdio>
  <command interpreter="python">gbk-feature-to-svg.py
--galaxy
--outfile_supporting $__new_file_path__
--gbk "${gbk}"

--plot "${plot}"

--plot_files_path "${plot.files_path}"

--plot_format "TXT"

--plot_id "${plot.id}"

</command>
  <inputs>
    <param help="Genbank file to plot" label="gbk" name="gbk" optional="False" type="data"/>
  </inputs>
  <outputs>
    <data format="svg" name="plot">
    </data>
  </outputs>
  <help>Print Features as Colored Boxes
===============================

c.f. Alan Davidson's software
</help>
  <tests/>
</tool>
