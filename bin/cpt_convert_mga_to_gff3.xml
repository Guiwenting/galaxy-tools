<?xml version="1.0"?>
<tool id="edu.tamu.cpt.generic.mga-to-gff3" name="MGA to GFF3" version="1.0">
  <description>convert formats</description>
  <version_command>python cpt_convert_mga_to_gff3.py --version</version_command>
  <stdio>
    <exit_code level="fatal" range="1:"/>
  </stdio>
  <command interpreter="python">cpt_convert_mga_to_gff3.py
--galaxy
--outfile_supporting $__new_file_path__
--file "${file}"

--data "${data}"

--data_files_path "${data.files_path}"

--data_format "TXT"

--data_id "${data.id}"

</command>
  <inputs>
    <param help="MGA Output" label="file" name="file" optional="False" type="data"/>
  </inputs>
  <outputs>
    <data format="gff3" name="data">
    </data>
  </outputs>
  <help>Convert MetaGeneAnnotator Table to GFF3
=======================================
</help>
  <tests/>
</tool>
