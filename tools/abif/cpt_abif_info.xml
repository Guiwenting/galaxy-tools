<?xml version="1.0"?>
<tool id="edu.tamu.cpt.abif.info" name="ABIF Info" version="1.0">
  <description>Dump information about AB1 file</description>
  <stdio>
    <exit_code level="fatal" range="1:"/>
  </stdio>
  <version_command>python cpt_abif_info.py --version</version_command>
  <command interpreter="python"><![CDATA[cpt_abif_info.py
$positional_1
> $default]]></command>
  <inputs>
    <param label="ABIF/AB1 file" name="positional_1" type="data"/>
  </inputs>
  <outputs>
    <data format="tabular" name="default"/>
  </outputs>
  <help><![CDATA[
**What it does**

Dumps all information available from an ABIF/AB1 file.

      ]]></help>
</tool>
