<?xml version="1.0"?>
<tool id="edu.tamu.cpt2.464.2016.password_passthrough" name="464 Password Passthrough" version="1.0">
  <description></description>
  <stdio>
    <exit_code level="fatal" range="1:"/>
  </stdio>
  <command><![CDATA[
python $__tool_directory__/password_passthrough.py
"$__user_email__"
"$password"
&&
cp $input_file $output_file;
]]></command>
  <inputs>
    <param label="Password" name="password" type="text" />
    <param label="Data File" name="input_file" type="data"/>
  </inputs>
  <outputs>
    <data format_source="input_file" name="output_file" label="$input_file.name"/>
  </outputs>
  <help><![CDATA[
**What it does**

Ensure that users were verified before continuing.
]]></help>
</tool>
