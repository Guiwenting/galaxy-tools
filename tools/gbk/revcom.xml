<?xml version="1.0"?>
<tool id="edu.tamu.cpt.genbank.RevCom" name="Reverse and Complement Genbank Files" version="1.0">
    <description></description>
  <macros>
    <import>macros.xml</import>
  </macros>
  <expand macro="requirements"/>
  <command interpreter="python" detect_errors="aggressive">revcom.py
  $file
> $genbank
</command>
  <inputs>
    <param label="Genbank file" name="file" type="data"/>
  </inputs>
  <outputs>
    <data format="txt" name="genbank">
    </data>
  </outputs>
  <help>
Reverse and Complement
======================

</help>
<citations />
</tool>
