<?xml version="1.0"?>
<macros>
  <xml name="requirements">
    <requirements>
      <requirement type="package" version="2.7">python</requirement>
      <requirement type="package" version="1.65">biopython</requirement>
      <requirement type="package" version="0.6.2">bcbiogff</requirement>
      <yield/>
    </requirements>
  </xml>
  <xml name="citations">
      <citations>
      </citations>
  </xml>
  <xml name="ldap_ref"
    token_name="dn_ref"
    token_label="Pick a DN"
    token_fromfile="ldap_people.loc">
        <repeat name="repeat_@NAME@" title="@LABEL@">
          <param name="@NAME@" label="Select a @LABEL@" type="select">
            <options from_file="@FROMFILE@">
                <column name="name" index="0"/>
                <column name="value" index="1"/>
            </options>
          </param>
        </repeat>
    </xml>
  <xml name="ldap_ref_single"
    token_name="dn_ref"
    token_label="Pick a DN"
    token_fromfile="ldap_people.loc">
          <param name="@NAME@" label="Select a @LABEL@" type="select">
            <options from_file="@FROMFILE@">
                <column name="name" index="0"/>
                <column name="value" index="1"/>
            </options>
          </param>
    </xml>
</macros>
